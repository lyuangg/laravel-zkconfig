{
    "name": "yuancode/laravel-zkconfig",
    "description": "laravel-zkconfig is a laravel configuration management tool",
    "type": "library",
    "require": {
        "php": "^7.2",
        "illuminate/console": "^5.5|^6.0|^7.0",
        "illuminate/container": "^5.5|^6.0|^7.0",
        "illuminate/contracts": "^5.5|^6.0|^7.0"
    },
    "require-dev": {
        "laravel/framework": "^5.5|^6.0|^7.0",
        "laravel/lumen-framework": "^5.5|^6.0|^7.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "yuan",
            "email": "yuancode@foxmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
          "Yuancode\\ZkConfig\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yuancode\\ZkConfig\\ZkConfigServiceProvider"
            ]
        }
    }
}
